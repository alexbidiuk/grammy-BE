{
  "name": "Grammy",
  "version": "1.0.0",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start": "npm run start:prod",
    "start:prod": "ts-node ./src/main.ts"
  },
  "dependencies": {
    "@nestjs/common": "^5.0.0-rc.3",
    "@nestjs/core": "^5.0.0-rc.3",
    "@nestjs/microservices": "^5.0.0-rc.3",
    "@nestjs/mongoose": "^3.0.1",
    "@nestjs/swagger": "^1.3.0",
    "@nestjs/testing": "^5.0.0-rc.3",
    "@nestjs/websockets": "^5.0.0-rc.3",
    "@types/jsonwebtoken": "^7.2.7",
    "class-transformer": "^0.1.7",
    "class-validator": "^0.7.2",
    "fs-extra": "^6.0.1",
    "jsonwebtoken": "^8.2.1",
    "mongoose": "^5.0.6",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "typescript": "^2.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.0.39",
    "@types/jest": "^21.1.8",
    "@types/node": "^9.3.0",
    "@types/supertest": "^2.0.4",
    "@types/mongoose": "^5.0.2",
    "jest": "^21.2.1",
    "nodemon": "^1.14.1",
    "prettier": "^1.11.1",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.4",
    "ts-node": "^6.0.0",
    "tsconfig-paths": "^3.1.1",
    "tslint": "^5.9.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
